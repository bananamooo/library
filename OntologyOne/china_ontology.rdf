    <rdf:Description rdf:about="http://www.globaltech.com/emp">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.globaltech.com/c">
