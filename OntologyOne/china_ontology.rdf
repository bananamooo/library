    </Ontology>

    <AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>

    <ObjectProperty rdf:about="http://www.globaltech.com/china#hasAvailableRole">
        <inverseOf rdf:resource="http://www.globaltech.com/china#isRoleInDepartment"/>
        <rdfs:label>有部门职位</rdfs:label>
    </ObjectProperty>

    <ObjectProperty rdf:about="http://www.globaltech.com/china#hasWorkerPosition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/china#CN_Employee"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/china#CN_JobPosition"/>
        <rdfs:label>员工职位</rdfs:label>
    </ObjectProperty>

    <ObjectProperty rdf:about="http://www.globaltech.com/china#isRoleInDepartment">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/china#CN_JobPosition"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/china#CN_Department"/>
        <rdfs:label>职位所属部门</rdfs:label>
    </ObjectProperty>

    <ObjectProperty rdf:about="http://www.globaltech.com/china#isWorkerInDepartment">
        <rdfs:domain rdf:resource="http://www.globaltech.com/china#CN_Employee"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/china#CN_Department"/>
        <propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.globaltech.com/china#hasWorkerPosition"/>
            <rdf:Description rdf:about="http://www.globaltech.com/china#isRoleInDepartment"/>
        </propertyChainAxiom>
        <rdfs:label>员工所属部门</rdfs:label>
    </ObjectProperty>

    <ObjectProperty rdf:about="http://www.globaltech.com/china#isWorkerOfCountry">
