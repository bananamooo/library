<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.globaltech.com/germany#"
     xml:base="http://www.globaltech.com/germany"
     xmlns:de="http://www.globaltech.com/germany#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">
    <owl:Ontology rdf:about="http://www.globaltech.com/germany">
        <owl:versionIRI rdf:resource="http://www.globaltech.com/germany/1.0.0"/>
        <rdfs:label>Human resource ontology for German employees</rdfs:label>
    </owl:Ontology>

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>

    <owl:ObjectProperty rdf:about="http://www.globaltech.com/germany#hasAvailablePosition">
        <owl:inverseOf rdf:resource="http://www.globaltech.com/germany#isJobPositionInDepartment"/>
        <rdfs:label>hat freie Stelle</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.globaltech.com/germany#hasJobPosition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/germany#DE_JobPosition"/>
        <rdfs:label>hat Job Position</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.globaltech.com/germany#isJobPositionInDepartment">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_JobPosition"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/germany#DE_Department"/>
        <rdfs:label>ist Position in der Abteilung</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.globaltech.com/germany#worksInCountry">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/germany#DE_Country"/>
        <rdfs:label>arbeitet im Land</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.globaltech.com/germany#worksInDepartment">
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/germany#DE_Department"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.globaltech.com/germany#hasJobPosition"/>
            <rdf:Description rdf:about="http://www.globaltech.com/germany#isJobPositionInDepartment"/>
        </owl:propertyChainAxiom>
        <rdfs:label>arbeitet in der Abteilung</rdfs:label>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="http://www.globaltech.com/germany#age">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.globaltech.com/germany#firstName">
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>Vorname des Mitarbeiters</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.globaltech.com/germany#id">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>Mitarbeiter-ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.globaltech.com/germany#lastName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>Nachname des Mitarbeiters</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.globaltech.com/germany#monthlySalary">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.globaltech.com/germany#startDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label>Einstellungsdatum des Mitarbeiters</rdfs:label>
    </owl:DatatypeProperty>

    <owl:Class rdf:about="http://www.globaltech.com/germany#DE_Country">
        <rdfs:label>Land</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.globaltech.com/germany#DE_Department">
        <rdfs:label>Abteilung</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.globaltech.com/germany#DE_Employee">
        <owl:hasKey rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.globaltech.com/germany#id"/>
        </owl:hasKey>
        <rdfs:label>Mitarbeiter</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.globaltech.com/germany#DE_JobPosition">
        <rdfs:label>Position</rdfs:label>
    </owl:Class>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#AnnaSchmidt">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <hasJobPosition rdf:resource="http://www.globaltech.com/germany#ITSolutionsSpecialist"/>
        <worksInCountry rdf:resource="http://www.globaltech.com/germany#Germany"/>
        <age rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">28</age>
        <firstName>Anna</firstName>
        <id>DE_20200930_001</id>
        <lastName>Schmidt</lastName>
        <monthlySalary rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">6000</monthlySalary>
        <startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-09-30</startDate>
        <rdfs:label>Anna Schmidt</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#Germany">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_Country"/>
        <rdfs:label>Germany</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#HR">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_Department"/>
        <hasAvailablePosition rdf:resource="http://www.globaltech.com/germany#HRManager"/>
        <rdfs:label>HR-Abteilung</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#HRManager">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_JobPosition"/>
        <isJobPositionInDepartment rdf:resource="http://www.globaltech.com/germany#HR"/>
        <rdfs:label xml:lang="en">HR Manager</rdfs:label>
        <rdfs:label xml:lang="de">HR-Manager</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#IT">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_Department"/>
        <hasAvailablePosition rdf:resource="http://www.globaltech.com/germany#ITSolutionsSpecialist"/>
        <rdfs:label>IT-Abteilung</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#ITSolutionsSpecialist">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_JobPosition"/>
        <isJobPositionInDepartment rdf:resource="http://www.globaltech.com/germany#IT"/>
        <rdfs:label xml:lang="en">IT Solutions Specialist</rdfs:label>
        <rdfs:label xml:lang="de">IT-Spezialist</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.globaltech.com/germany#MaxMustermann">
        <rdf:type rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
        <hasJobPosition rdf:resource="http://www.globaltech.com/germany#HRManager"/>
        <worksInCountry rdf:resource="http://www.globaltech.com/germany#Germany"/>
        <age rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">39</age>
        <firstName>Max</firstName>
        <id>DE_20210115_001</id>
        <lastName>Mustermann</lastName>
        <monthlySalary rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">8100</monthlySalary>
        <startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-01-15</startDate>
        <rdfs:label>Max Mustermann</rdfs:label>
    </owl:NamedIndividual>

    <rdf:Description rdf:about="http://www.globaltech.com/c">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.globaltech.com/emp">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment></rdfs:comment>
        <rdfs:label>Assign all employees to Country=Germany</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.globaltech.com/germany#DE_Country"/>
                        <swrl:argument1 rdf:resource="http://www.globaltech.com/c"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                <swrl:classPredicate rdf:resource="http://www.globaltech.com/germany#DE_Employee"/>
                                <swrl:argument1 rdf:resource="http://www.globaltech.com/emp"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="http://www.globaltech.com/germany#worksInCountry"/>
                        <swrl:argument1 rdf:resource="http://www.globaltech.com/emp"/>
                        <swrl:argument2 rdf:resource="http://www.globaltech.com/c"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
</rdf:RDF>

