    <owl:ObjectProperty rdf:about="http://www.globaltech.com/employee#hasAlignedRole">
        <rdfs:domain rdf:resource="http://www.globaltech.com/employee#Employee"/>
        <rdfs:range rdf:resource="http://www.globaltech.com/employee#AlignedRole"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.globaltech.com/employee#hasLocalRole"/>
            <rdf:Description rdf:about="http://www.globaltech.com/employee#mapsToAlignedRole"/>
        </owl:propertyChainAxiom>
        <rdfs:label>Aligned role (i.e. organisation level role) held by an employee</rdfs:label>
    </owl:ObjectProperty>

